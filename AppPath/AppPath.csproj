<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net8.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <AssemblyName>Openus.[APP_NAME].$(MSBuildProjectName)</AssemblyName>
    <RootNamespace>Openus.[APP_NAME].$(MSBuildProjectName.Replace(" ", "_"))</RootNamespace>
    <Authors>elecbug</Authors>
    <Company>Openus.NET</Company>
    <Copyright>Openus.NET</Copyright>
    <RepositoryUrl>https://github.com/openus-dotnet/[APP_NAME]</RepositoryUrl>
  </PropertyGroup>

</Project>
